
    
    create table ASTRONAUTS (
       ASTRONAUT_ID integer not null auto_increment,
        fName varchar(255),
        lName varchar(255),
        missionCount integer not null,
        rank varchar(255),
        spaceWalksCount integer not null,
        primary key (ASTRONAUT_ID)
    ) engine=InnoDB

    
    create table CARGOS (
       CARGO_ID integer not null auto_increment,
        description varchar(255),
        type varchar(255),
        TRANSPORT_FLIGHT_ID integer,
        primary key (CARGO_ID)
    ) engine=InnoDB

    
    create table MISSION_SPACECRAFTS (
       MISSION_ID integer not null,
        SPACECRAFT_ID integer not null,
        primary key (MISSION_ID, SPACECRAFT_ID)
    ) engine=InnoDB

    
    create table MISSIONS (
       MISSION_ID integer not null auto_increment,
        description varchar(255),
        destination varchar(255),
        primary key (MISSION_ID)
    ) engine=InnoDB

    
    create table MISSIONS_ASTRONAUTS (
       MISSION_ID integer not null,
        ASTRONAUT_ID integer not null,
        primary key (MISSION_ID, ASTRONAUT_ID)
    ) engine=InnoDB

    
    create table RESEARCH_MISSIONS (
       RESEARCH_MISSION_ID integer not null,
        primary key (RESEARCH_MISSION_ID)
    ) engine=InnoDB

    
    create table ROLES (
       ROLE_ID integer not null auto_increment,
        role varchar(255),
        ASTRONAUT_ID integer,
        MISSION_ID integer,
        primary key (ROLE_ID)
    ) engine=InnoDB

    
    create table SPACECRAFTS (
       SPACECRAFT_ID integer not null auto_increment,
        capacity integer not null,
        name varchar(255),
        numberOfCrewMembers integer not null,
        yearOfProduction integer not null,
        primary key (SPACECRAFT_ID)
    ) engine=InnoDB

    
    create table TASKS (
       TASK_ID integer not null auto_increment,
        description varchar(255),
        name varchar(255),
        RESEARCH_MISSION_ID integer,
        primary key (TASK_ID)
    ) engine=InnoDB

    
    create table TRANSPORT_FLIGHTS (
       TRANSPORT_FLIGHT_ID integer not null,
        primary key (TRANSPORT_FLIGHT_ID)
    ) engine=InnoDB

    
